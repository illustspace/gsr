{
  "name": "@geospatialregistry/indexer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn db:start && next dev",
    "build": "yarn db:generate && next build",
    "serve": "next start",
    "lint": "next lint",
    "db:start": "docker-compose up -d db",
    "db:stop": "docker-compose stop db",
    "db:migrate": "prisma migrate dev --name",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "e2e": "yarn e2e:db:start && yarn e2e:start",
    "e2e:start": "NODE_ENV=test PORT=3001 yarn start",
    "e2e:build": "NODE_ENV=test PORT=3001 yarn build",
    "e2e:serve": "NODE_ENV=test PORT=3001 yarn serve",
    "e2e:db:start": "docker-compose up -d e2e-db && yarn e2e:db:deploy",
    "e2e:db:stop": "docker-compose stop e2e-db && docker-compose rm -vf e2e-db",
    "e2e:db:deploy": "dotenv -e .env.test -- yarn prisma migrate deploy",
    "e2e:db:studio": "dotenv -e .env.test -- prisma studio"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.9",
    "@chakra-ui/react": "^2.3.1",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@ethersproject/abi": "^5.4.1",
    "@ethersproject/bignumber": "^5.4.1",
    "@ethersproject/contracts": "^5.4.1",
    "@ethersproject/keccak256": "^5.4.0",
    "@ethersproject/providers": "^5.4.5",
    "@ethersproject/strings": "^5.4.0",
    "@ethersproject/wallet": "^5.4.0",
    "@geospatialregistry/sdk": "^0.0.0",
    "@mapbox/mapbox-gl-geocoder": "^5.0.1",
    "@prisma/client": "^4.2.1",
    "@types/geojson": "^7946.0.10",
    "@types/mapbox-gl": "^2.7.5",
    "@types/mapbox__mapbox-gl-geocoder": "^4.7.3",
    "@walletconnect/web3-provider": "^1.8.0",
    "axios": "^0.27.2",
    "framer-motion": "^7.2.1",
    "mapbox-gl": "^2.10.0",
    "next": "12.2.5",
    "ngeohash": "^0.6.3",
    "prisma": "^4.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.34.2",
    "react-map-gl": "^7.0.19"
  },
  "devDependencies": {
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "node-mocks-http": "^1.11.0",
    "typescript": "4.8.2"
  }
}
